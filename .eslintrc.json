{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"globals": {
		"__MAPBOX_TOKEN__": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module",
		"ecmaFeatures": {
			"modules": true,
			"impliedStrict": true,
			"jsx": true
		}
	},
	"overrides": [
		{
			"files": [
				"**/*.jsx"
			]
		}
	],
	"rules": {
		"no-unused-vars": "off"
	}
}
